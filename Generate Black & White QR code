import qrcode as qr

img = qr.make("I'm a programmer & I love to do it all day")
img.save("Master_NB.png")


#For User


import qrcode as qr

print('Enter your qr code information:')
info = input()
print('Enter your file name:')
filename = input()
print('Enter your file format:')
format=input()

img=qr.make(info)
img.save(f'{filename}.{format}')
